<!--
Download infra-red satellite image over Europe
for the following timestamp
  2009/07/21 at 11h00
  2009/07/21 at 12h00
  2009/07/21 at 13h00
-->
<request> 
  <type>getData</type> 
  <database>imgdb</database> 
  <format>tiff</format> 
  <variables> 

    <timeStampList>
      <timeStamp>20090721110000</timeStamp>
      <timeStamp>20090721120000</timeStamp>
      <timeStamp>20090721130000</timeStamp>
    </timeStampList>

    <product>METEOSAT</product>
    <domain>ATL-EUROPE</domain>
    <param>IR108</param>

    <!--
    <previewArea>TOULOUSE</previewArea>
    -->
    </variables>
</request>
